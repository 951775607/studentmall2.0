<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
		PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
		"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lhq.studentmall.dao.AreaDao">
	<select id="queryArea" resultType="com.lhq.studentmall.entity.Area">
		SELECT area_id,area_name,priority,create_time,last_edit_time
		FROM tb_area
		ORDER BY
		priority DESC
	</select>

	<!--以下-->
</mapper>